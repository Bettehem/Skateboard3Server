{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Skateboard3Context": "Data Source=skateboard3server.db"
  },
  "Skateboard": {
    "Blaze": {
      "PublicHost": "localhost",
      "PublicIp": "127.0.0.1",
      "Qos": {
